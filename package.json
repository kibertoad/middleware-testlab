{
  "name": "middleware-testlab",
  "version": "2.5.1",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest --config=jest.config.json",
    "test:ci": "npm run eslint && npm run test",
    "test:update-snaphost": "jest --config=jest.config.json --updateSnapshot",
    "eslint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\""
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/serialize-error": "2.1.0",
    "@types/koa": "^2.0.52",
    "@types/koa-router": "^7.0.42",
    "express": "^4.17.1",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "serialize-error": "3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^10.17.5",
    "@types/supertest": "^2.0.8",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ],
  "homepage": "http://github.com/kibertoad/middleware-testlab",
  "keywords": [
    "koa",
    "express",
    "middleware",
    "test",
    "testing"
  ]
}
