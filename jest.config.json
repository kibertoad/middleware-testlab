{
  "moduleFileExtensions": [
    "ts",
    "js",
    "json"
  ],
  "testMatch": [
    "<rootDir>/**/?(*.)(spec|test).ts|tsx"
  ],
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "testEnvironment": "node",
  "reporters": [ "default" ],
  "collectCoverage": false,
  "coveragePathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/coverage/"
  ]
}
